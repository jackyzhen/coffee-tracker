{
  "extends": "airbnb",
  "rules": {
    "eol-last": 0,
    "max-len": 0,
    "new-cap": 1,
    "no-shadow": 0,
    "no-unused-expressions": 0,
    "no-param-reassign": [2, {
      "props": false
    }],
    "import/no-unresolved": [2, {
      "ignore": [".\/styles$"]
    }],
    "import/no-extraneous-dependencies": ["error", { "devDependencies": true }],
    "arrow-body-style": 0,
    "linebreak-style": 0
  },
  "globals": {
    // Mocha
    "describe": true,
    "it": true
  }
}